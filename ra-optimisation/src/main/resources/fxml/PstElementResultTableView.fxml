<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="remedialActionElementsBorderPane" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <Label text="%PstElement" BorderPane.alignment="CENTER_LEFT">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
        </Label>
    </top>
    <center>
<TableView fx:id="pstElementTable">
    <columns>
        <TableColumn fx:id="idTabColumn" prefWidth="200.0" text="%ID" />
        <TableColumn fx:id="preOptimisationAngleTabColumn" prefWidth="100.0" text="%PreOptimisationAngle" />
            <TableColumn fx:id="preOptimisationTapPositionTabColumn" prefWidth="100.0" text="%PreOptimisationTapPosition" />
        <TableColumn fx:id="postOptimisationAngleTabColumn" prefWidth="100.0" text="%PostOptimisationAngle" />
            <TableColumn fx:id="postOptimisationTapPositionTabColumn" prefWidth="100.0" text="%PostOptimisationTapPosition" />
    </columns>
</TableView>
    </center>
</BorderPane>
